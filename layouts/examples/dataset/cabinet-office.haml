!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "content-type"}/
    %title Principle areas of Cabinet office spending 2010/2011
    %link{:href => "/stylesheets/examples/dataset/cabinet-office.css", :rel => "stylesheet"}
  %body

    = yield

    %script{:src => "/js/jquery.min.js", :type => "text/javascript"}
    %script{:src => "/js/lodash.js", :type => "text/javascript"}
    %script{:src => "/js/backbone.min.js", :type => "text/javascript"}
    %script{:src => "/js/miso.ds.deps.js", :type => "text/javascript"}
    %script{:src => "/js/d3/d3.v2.min.js", :type => "text/javascript"}
    %script{:src => "/js/accounting.min.js", :type => "text/javascript"}
    %script{:src => "/js/examples/dataset/cabinet-office.js"}
    :plain
      <script type="text/template" id="grouping">
        <% _.each(columns, function(column) { %>
          <option value="<%= column.name %>"><%= column.name %></option>
        <% }); %>
      </script>

      <script type="text/template" id="dateRanges">
        <% _.each(dateRanges, function(range) { %>
          <option value="<%= range %>"><%= range %></option>
        <% }); %>
      </script>